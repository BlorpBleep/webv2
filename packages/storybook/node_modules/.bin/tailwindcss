#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules/tailwindcss/lib/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules/tailwindcss/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules/tailwindcss/lib/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules/tailwindcss/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/tailwindcss@3.4.10_ts-node@10.9.2_@swc+core@1.7.24_@swc+helpers@0.5.13__@types+node@20.5.1_typescript@4.9.5_/node_modules:/Users/davidawatere/Documents/GitHub/webv2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
